{
  "version": 2,
  "builds": [
    {
      "src": "skillup24/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "skillup24/wsgi.py"
    }
  ],
  "cache": false
}
