{
    "version": 2,
    "builds": [
        {
            "src": "skillup24/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb", 
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": { "cache-control": "max-age=31536000, immutable" },
            "dest": "/static/$1",
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "skillup24/wsgi.py"
        }
    ]
}
